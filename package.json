{
  "name": "sample-app-web",
  "version": "1.0.0",
  "description": "This is the Sauce Labs Sample Application which is designed to be used from desktop web browsers",
  "scripts": {
    "lint": "eslint . --ext .js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "prepush": "npm run lint",
    "test": "node scripts/test.js --verbose --no-watch",
    "test:watch": "node scripts/test.js --verbose",
    "test.e2e.local.chrome": "wdio e2e/configs/wdio.local.chrome.conf.js",
    "test.e2e.sauce.all": "wdio e2e/configs/wdio.sauce.all.browsers.conf.js",
    "storybook": "start-storybook -p 6006",
    "serve": "npm run build && node scripts/createServeFile.js && serve -s build",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saucelabs/sample-app-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saucelabs/sample-app-web/issues"
  },
  "homepage": ".",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@material-ui/core": "^3.9.3",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "classnames": "^2.2.6",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@wdio/appium-service": "^5.9.3",
    "@wdio/cli": "^5.9.6",
    "@wdio/jasmine-framework": "^5.9.4",
    "@wdio/local-runner": "^5.9.6",
    "@wdio/sauce-service": "^5.9.3",
    "@wdio/selenium-standalone-service": "^5.9.3",
    "@wdio/spec-reporter": "^5.9.3",
    "@wdio/sync": "^5.9.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-jest": "^22.6.4",
    "history": "^4.9.0",
    "husky": "^2.3.0",
    "react-test-renderer": "^16.8.6",
    "serve": "^11.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!./node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 99,
        "functions": 99,
        "lines": 99,
        "statements": 99
      }
    },
    "testURL": "http://localhost",
    "coverageDirectory": "./.coverage",
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/utils/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|png)$": "<rootDir>/src/utils/__mocks__/fileMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
